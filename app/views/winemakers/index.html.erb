<p id="notice"><%= notice %></p>

<h1>Winemakers</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Age</th>
      <th>Country</th>
      <th>Position</th>
      <th>Magazine</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @winemakers.each do |winemaker| %>
      <tr>
        <td><%= winemaker.name %></td>
        <td><%= winemaker.age %></td>
        <td><%= winemaker.country %></td>
        <td><%= winemaker.position.name %></td>
        <td><%= winemaker.magazine.name %></td>
        <td><%= link_to 'Show', winemaker %></td>
        <td><%= link_to 'Edit', edit_winemaker_path(winemaker) %></td>
        <td><%= link_to 'Destroy', winemaker, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Winemaker', new_winemaker_path %>
